<launch>
  <node pkg="uvc_camera" name="camera_node" type="uvc_camera_node">
    <param name="device" type="string" value="/dev/video0"/>
    <param name="width"  type="int"    value="1920"/>
    <param name="height" type="int"    value="1080"/>
    <param name="pixel_format"         value="mjpg"/>
  </node>

  <node pkg="rmrc_hazmat" name="hazmat_node" type="hazmat_classification.py"/>

  <node pkg="rviz" name="rviz" type="rviz"/>
</launch>
